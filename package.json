{
	"name": "adalab-web-starter-kit",
	"version": "4.0.0",
	"description": "Adalab web starter kit",
	"type": "module",
	"devDependencies": {
		"@vituum/vite-plugin-concat": "^1.1.1",
		"sass": "^1.83.4",
		"sharp": "^0.34.4",
		"vite": "^7.1.9",
		"vite-plugin-html-inject": "^1.1.2",
		"vite-plugin-image-optimizer": "^1.1.7"
	},
	"scripts": {
		"start": "vite",
		"dev": "vite",
		"build": "vite build --emptyOutDir",
